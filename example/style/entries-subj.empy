@%stylename        "Entries (subject)"
@%styleauthor      "Decklin Foster"
@%styleemail       "decklin@red-bean.com"
@%styledescription "Entries as subject links."
@
@{import time}@
@
@[for e in entries]@
<p>@(time.strftime('%a %d %b, %H:%M:%S', e.date)): @
<a href="@base/@`e.year`/@`e.month`/@`e.day`/@`e.subject`.html">@
@e.subject</a></p>

@[end for]@
