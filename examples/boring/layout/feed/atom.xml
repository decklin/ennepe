__layoutname__ = "Feed"
__layoutauthor__ = "Decklin Foster <decklin@red-bean.com>"
__layoutdescription__ = "Feed of all entries in the last week."

import time

def newer(e, ago):
    return time.mktime(e.mtime) > (time.time() - ago)

entries = [e for e in entries if newer(e, 86400*7)][-10:]
entries.reverse()

atom = muse.template(name='atom', **locals())

write(atom.serialize())
